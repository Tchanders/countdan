{
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery"
	],

	"globals": {
		"Countdan": "readonly"
	},

	"rules": {
		"max-len": "off",
		"no-bitwise": "off",
		"no-implicit-globals": "off",
		"no-jquery/no-class-state": "off",
		"no-jquery/no-global-selector": "off",
		"no-use-before-define": [ "error", "nofunc" ]
	}
}
